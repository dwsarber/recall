{
  "manifest_version": 2,

  "name": "Recall",
  "description": "Opens a new tab to a random bookmark for the current browser profile.",
  "version": "1.0",

  "background": {
    "scripts": ["main.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open a random bookmark"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ]
}
